<div class="page-header">
  <h1>
    Users
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Created at</th>
        <th>Sign in Count</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>        
        <td><%= user.created_at %></td>
        <td><%= user.sign_in_count %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>